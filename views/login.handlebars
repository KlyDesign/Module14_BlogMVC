<div class="row justify-content-center">
  <div class="col-md-6">
    <h2 class="m-2">Login</h2>
    <form class="form login-form m-4">
        <div class="mb-3">
            <label for="log-email" class="form-label">Email:</label>
            <input type="text" class="form-control" id="log-email">
        </div>
        <div class="mb-3">
            <label for="log-password" class="form-label">Password:</label>
            <input type="password"  autocomplete="current-password" class="form-control" id="log-password">
        </div>
        <div class="form-group">
            <button id="logSubmit"  class="btn btn-primary" type="submit">Submit</button>
        </div>
    </form>
  </div>
  <div class="col-md-6">
    <h2 class="m-2">Signup</h2>
    <form class="form signup-form m-4">
        <div class="mb-3">
            <label for="reg-name" class="form-label">Username:</label>
            <input type="text" class="form-control" id="reg-name">
        </div>
        <div class="mb-3">
            <label for="reg-email" class="form-label">Email:</label>
            <input type="text" class="form-control" id="reg-email">
        </div>
        <div class="mb-3">
            <label for="reg-password" class="form-label">Password:</label>
            <input type="password"  autocomplete="current-password" class="form-control" id="reg-password">
        </div>
        <div class="form-group">
            <button id="regSubmit" class="btn btn-primary" type="submit">Submit</button>
        </div>
    </form>
  </div>
</div>


<script src="../login.js"></script>